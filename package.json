{
  "name": "@vinicunca/perkakas",
  "type": "module",
  "version": "0.5.4",
  "packageManager": "pnpm@9.1.4",
  "description": "Utility for JavaScript and Typescript.",
  "author": "praburangki<https://github.com/praburangki>",
  "license": "MIT",
  "homepage": "https://github.com/vinicunca/perkakas#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicunca/perkakas.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/perkakas/issues"
  },
  "keywords": [
    "javascript utility",
    "typescript utility",
    "javascript utilities",
    "typescript utilities",
    "javascript helpers",
    "typescript helpers"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup",
    "prepublish": "pnpm run build",
    "typedoc": "typedoc --json dist/metadata.json",
    "dev": "pnpm build --watch",
    "lint": "eslint . --fix",
    "publish:ci": "pnpm publish --access public --no-git-checks",
    "release": "bumpp package.json",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typeCheck": "tsc",
    "typeCheck:dist": "tsc --project tsconfig.dist.json"
  },
  "dependencies": {
    "type-fest": "^4.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@vinicunca/eslint-config": "^2.6.1",
    "@vitest/coverage-v8": "^1.6.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.4.0",
    "prettier": "^3.3.1",
    "tsup": "^8.1.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
